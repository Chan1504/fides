{
	"name": "fidesctl-config-parser",
	"displayName": "Fidesctl Config Parser",
	"description": "Valides fidesctl config files",
	"publisher": "earmenda-fidesctl-publisher",
	"version": "0.0.5",
	"repository": {
		"type": "git",
		"url": "git://github.com/ethyca/fides.git"
	},
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:fidesctl-config-parser.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "fidesctl-config-parser.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"package": "mkdirp dist && vsce package -o dist/fidesctl-config-parser.vsix",
		"deploy": "vsce publish -i dist/fidesctl-config-parser.vsix",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"deployed-version": "vsce show earmenda-fidesctl-publisher.fidesctl-config-parser | grep \"Version:\" | awk -F \":\" '{print $2}' | xargs"
	},
	"devDependencies": {
		"@types/vscode": "^1.60.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2",
		"vsce": "^1.100.1"
	}
}
